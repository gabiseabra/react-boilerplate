{
	"presets": [
		"es2015",
		"stage-1",
		"react"
	],
	"plugins": [
		["webpack-alias", {"config": "webpack/bundles/server.babel.js"}]
	],
	"env": {
		"production": {
			"plugins": [
				"transform-react-remove-prop-types",
				"transform-react-constant-elements",
				"transform-react-inline-elements"
			]
		},
		"development": {
			"plugins": [
				"react-hot-loader/babel"
			]
		}
	}
}
